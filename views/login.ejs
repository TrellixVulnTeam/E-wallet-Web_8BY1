<%- include('./layouts/head.ejs') %>

    <body class="gradient-custom-2">
        <section class="h-100 loginform">
            <div class="container py-5 h-100">
                <div class="row d-flex justify-content-center align-items-center h-100">
                    <div class="col-xl-10">
                        <div class="card rounded-3 text-black">
                            <div class="row g-0">
                                <div class="col-lg-6">
                                    <div class=" p-md-5 mx-md-4">
                                        <div class="text-center">
                                            <img src="data:image/png;base64,"
                                                style="width: 185px; height: 150px;" alt="logo">
                                            <h4 class="mt-1 mb-5 pb-1">Ví điện tử E-Wallet</h4>
                                        </div>

                                        <form action="/" method="post">
                                         
                                            <div class="form-group mb-4">
                                                <label class=" form-label " for="username">Username</label>
                                                <input value="<%= username %>" name="username" type="text" id="username" class="form-control" placeholder="Số tài khoản" />
                                                
                                            </div>

                                            <div class="form-group mb-4 ">
                                                <label class="form-label " for="password">Password</label>
                                                <input value="<%= password %>" name="password" type="password" id="password" placeholder="Nhập mật khẩu" class="form-control " />
                                                
                                            </div>

                                            <div class="text-center pt-1 mb-5 pb-1 ">
                                                <button class="btn btn-primary btn-block fa-lg gradient-custom mb-3 " type="button ">Đăng nhập</button>
                                                <a class="text-muted " href="/resetPassword">Quên mật khẩu?</a>
                                            </div>
                                            <% if(error){ %>
                                                <div class="alert alert-danger mb-3">
                                                    <strong>Lưu ý!</strong>
                                                    <%= error %>
                                                </div>
                                                <%}%>

                                                    <div class="d-flex align-items-center justify-content-center pb-4 ">
                                                        <p class="mb-0 me-2 ">Bạn chưa có tài khoản?</p>
                                                        <a href="/register" type="button " class="btn btn-outline-danger ">Tạo mới</a>
                                                    </div>
                                        </form>
                                    </div>
                                </div>

                                <div class="col-lg-6 d-flex align-items-center gradient-custom ">
                                    <div class="text-white px-3 py-4 p-md-5 mx-md-4 ">
                                        <h4 class="mb-4 ">Thành viên</h4>
                                        <p class="small mb-0 ">Trương Tấn Hùng - 51900083</p>
                                        <p class="small mb-0 ">Huỳnh Quang Huy - 51900088</p>
                                        <p class="small mb-0 ">Trần Minh Kha - 51900101</p>
                                        <p class="small mb-0 ">Lê Triệu Vỷ - 51900290</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </section>
        <script src="/Js/main.js"></script>
    </body>

    </html>
    <script>
        createAutoClosingAlert(".alert-danger", 2000);
    </script>